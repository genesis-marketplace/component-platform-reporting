{
  "name": "@genesislcap/pbc-reporting-genx",
  "description": "Genesis Reporting PBC Genx",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "genxSeedConfig": {
    "exclude": [
      "README.md",
      "package.json",
      "license.txt"
    ],
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".kt",
          ".kts",
          ".md"
        ]
      }
    }
  },
  "dependencies": {
    "@genesislcap/seed-utils": "~14.173.3",
    "tslib": "^2.3.1"
  }
}
